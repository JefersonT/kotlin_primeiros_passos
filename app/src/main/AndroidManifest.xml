        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".App">

        <receiver android:name=".AlarmReceicer">
        </receiver>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Cadastrar"></activity>
        <activity android:name=".MostrarMedida"></activity>
        <activity android:name=".NovaMassa"></activity>
        <activity android:name=".NovaAltura"></activity>
        <activity android:name=".ListaMedidas"></activity>
        <activity android:name=".Grafico"></activity>
        <activity android:name=".CadMedidas"></activity>

    </application>

</manifest>